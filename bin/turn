#!/usr/bin/env ruby
require_relative '../lib/turn'
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
display_board(board)
turn(board)

index = input_to_index(input)
puts index
if (valid_move?(board, index))
  move(board, index, current_player = "X")
else
  turn(board)
end